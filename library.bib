journal = {Applied Soil Ecology},
keywords = {Seasonal response,Soil microbial activity,Soil microbiology,Subsurface soils},
month = {jun},
number = {3},
pages = {171--181},
title = {{Surface and subsurface microbial biomass, community structure and metabolic activity as a function of soil depth and season}},
