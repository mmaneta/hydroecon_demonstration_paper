issn = {10035494},
journal = {Change},
number = {November},
pages = {12--17},
pmid = {17429376},
publisher = {IPCC},
series = {Synthesis Report},
title = {{Climate Change 2007 : An Assessment of the Intergovernmental Panel on Climate Change}},
url = {http://www.ipcc.ch/pdf/assessment-report/ar4/syr/ar4{\_}syr.pdf},
volume = {446},
year = {2007}
}
