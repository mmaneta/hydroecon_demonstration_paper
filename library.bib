author = {Ivanov, Valeriy Y and Fatichi, Simone and Jenerette, G Darrel and Espeleta, Javier F and Troch, Peter A and Huxman, Travis E},
doi = {10.1029/2009wr008611},
file = {:home/maneta/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ivanov et al. - 2010 - Hysteresis of soil moisture spatial heterogeneity and the homogenizing effect of vegetation.pdf:pdf},
