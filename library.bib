file = {:home/maneta/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gribovszki et al. - 2008 - Riparian zone evapotranspiration estimation from diurnal groundwater level fluctuations(3).pdf:pdf},
isbn = {0022-1694},
issn = {00221694},
journal = {Journal of Hydrology},
keywords = {Diurnal groundwater level fluctuations,Evapotranspiration,Riparian zone},
number = {1-2},
pages = {6--17},
title = {{Riparian zone evapotranspiration estimation from diurnal groundwater level fluctuations}},
volume = {349},
year = {2008}
}
