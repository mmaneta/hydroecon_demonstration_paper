keywords = {Atmospheric modeling,CO{\&}{\#}x2082,CO2fluxes,Carbon,Data models,Ecosystems,Soil Moisture Active Passive (SMAP) Mission,Soil moisture,Vegetation mapping,carbon cycle,ecosystems,environmental monitoring,fluxes,microwave remote sensing,soil moisture,vegetation,water cycle,water cycle.},
