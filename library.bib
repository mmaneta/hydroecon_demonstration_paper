author = {Vicuna, Sebastian and Dracup, John A and Lund, Jay R and Dale, Larry L and Maurer, Edwin P},
doi = {10.1029/2009wr007838},
issn = {0043-1397},
journal = {Water Resour. Res.},
keywords = {climate impacts water management 1807 Hydrology: C},
pages = {W04505 ST  -- Basin--scale water system operations w},
title = {{Basin-scale water system operations with uncertain future climate conditions: Methodology and case studies}},
