@article{Mendelsohn1996,
abstract = {The article presents a reply by the authors to a commentary on their paper entitled The Impact of Global Warming on Agriculture: A Ricardian Approach, published in the September 1994 issue of the American Economic Review. In our paper we developed an approach to measuring the impact of global warming on agriculture. We call this approach Ricardian because it relies upon standard rent theory as a way of identifying the impact of changes on net economic welfare. We compared the Ricardian approach with the traditional production-function approach, which uses agricultural production functions but has great difficulty identifying all the other adjustments that farmers make in response to changing external conditions. The Ricardian approach is particularly well-suited to a tremendously heterogenous sector because it can rely upon reduced-form estimation and does not require the impossible task of constructing structural models of hundreds of crops in thousands of locations. Our Ricardian approach assumes that the existing pattern of agricultural land rents and land prices reflects the long-run equilibrium economic effect of climate and other geophysical and economic variables. Standard economic reasoning shows that, by calculating the estimated effects of perturbing the climatic variables, we can project the impact of climate change on economic welfare.},
author = {Mendelsohn, Robert and Nordhaus, William},
doi = {10.2307/2118296},
file = {:home/maneta/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Mendelsohn, Nordhaus - 1996 - The Impact of Global Warming on Agriculture Reply.pdf:pdf},
