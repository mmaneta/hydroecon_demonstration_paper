file = {:home/maneta/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Tesemma et al. - 2015 - Including the dynamic relationship between climatic variables and leaf area index in a hydrological model to imp.pdf:pdf},
issn = {1607-7938},
journal = {Hydrology and Earth System Sciences},
number = {6},
pages = {2821--2836},
title = {{Including the dynamic relationship between climatic variables and leaf area index in a hydrological model to improve streamflow prediction under a changing climate}},
