journal = {Water Resources Research},
keywords = {http://dx.doi.org/10.1029/2007WR006051, doi:10.102},
number = {5},
pages = {1--15},
title = {{Global consumptive water use for crop production: The importance of green water and virtual water}},
volume = {45},
year = {2009}
}
