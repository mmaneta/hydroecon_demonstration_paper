isbn = {0168-1923},
issn = {01681923},
journal = {Agricultural and Forest Meteorology},
keywords = {Crop planting date,Freeze/thaw,GPP,Phenology,Spring frost,Vegetation index},
pages = {132--143},
publisher = {Elsevier B.V.},
title = {{Response of vegetation growth and productivity to spring climate indicators in the conterminous United States derived from satellite remote sensing data fusion}},
