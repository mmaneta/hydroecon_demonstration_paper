author = {Helming, K and Roth, Ch. H and Wolf, R and Diestel, H},
journal = {Soil Technology},
number = {3},
pages = {273--286},
title = {{Characterization of rainfall - microrelief interactions with runoff using parameters derived from digital elevation models (DEMs)}},
volume = {6},
year = {1993}
}
@article{Zhang2008,
