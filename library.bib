@article{Troy2015,
abstract = {Climate variability and extremes are expected to increase due to climate change; this may have$\backslash$r significant negative impacts for agricultural production. Previous work has primarily focused on the$\backslash$r impact of mean growing season temperature and precipitation on rainfed crop yields with little work$\backslash$r on irrigated crop yields or climate extremes and their timing. County-level crop yields and daily$\backslash$r precipitation and temperature data are pooled to quantify the impact of climate variability and$\backslash$r extremes on four major staple crops in the United States. Conditional density plots are used to$\backslash$r graphically explore the relationship between climate extremes and crop yields, thereby avoiding$\backslash$r assumptions about linearity or underlying probability distributions. Non-linear and threshold-type$\backslash$r relationships exist between yields and both precipitation and temperature climate indices;$\backslash$r irrigation significantly reduces the impact of all climate indices. In some cases, this occurs by$\backslash$r shifting the threshold, such that a more extreme weather event is necessary to negatively impact$\backslash$r yields. In other cases, irrigation essentially decouples the crop yields from climate. This work$\backslash$r demonstrates that irrigation may be a beneficial adaptation mechanism to changes in climate extremes$\backslash$r in coming decades.},
author = {Troy, T. J. and Kipgen, C. and Pal, I.},
