journal = {Water Resources Research},
keywords = {doi:10.1029/9,http://dx.doi.org/10.1029/98WR02532},
number = {12},
pages = {3303--3313},
title = {{Stability and damping in a dynamical model of hillslope hydrology}},
volume = {34},
year = {1998}
}
