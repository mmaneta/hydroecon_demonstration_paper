journal = {Journal of Hydrology},
keywords = {2015,2015/07,Albiac,Aquifer-river interactions,Climate change,Hydro-economic modeling,Kahil,Water policies,nacional,oa99,publicacion tecnica,res00},
pages = {510--524},
publisher = {Elsevier B.V.},
title = {{Hydro-economic modeling with aquifer-river interactions to guide sustainable basin management}},
url = {http://hdl.handle.net/10532/3018 http://dx.doi.org/10.1016/j.jhydrol.2016.05.057},
volume = {539},
year = {2016}
