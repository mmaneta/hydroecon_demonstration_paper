@article{Du2015a,
abstract = {An approach for deriving atmosphere total precipitable water vapor (PWV)$\backslash$nand surface air temperature over land using satellite passive microwave$\backslash$nradiometry from the Advanced Microwave Scanning Radiometer 2 (AMSR2) was$\backslash$ndeveloped in this study. The PWV algorithm is based on theoretical$\backslash$nanalysis and comparisons against similar retrievals from the Atmospheric$\backslash$nInfrared Sounder (AIRS). The AMSR2 PWV retrievals compare favorably with$\backslash$nAIRS operational PWV products (R-2 {\textgreater}= 0.80 and rmse: 4.4-5.6 mm) and$\backslash$nindependent PWV observations from the SuomiNet North American Global$\backslash$nPositioning System station network, with an overall mean rmse of 4.7 mm$\backslash$nand more than 78{\%} of absolute retrieval errors below 5 mm. The PWV$\backslash$nretrievals were then applied within an AMSR2 multifrequency brightness$\backslash$ntemperature algorithm for deriving atmosphere-corrected surface air$\backslash$ntemperatures. The estimated temperatures agree favorably (R-2 {\textgreater} 0.80 and$\backslash$nrmse {\textless} 3.5 K) with independent weather station daily air temperature$\backslash$nmeasurements spanning global climate and land cover variability. The$\backslash$nresulting PWV estimates increase surface air temperature retrieval$\backslash$naccuracy in our algorithm scheme. The AMSR2 algorithm is readily applied$\backslash$nto similar microwave sensors including the AMSR for EOS and provides$\backslash$nsuitable performance and accuracy to support hydrologic, ecosystem, and$\backslash$nclimate change studies.},
author = {Du, Jinyang and Kimball, John S. and Jones, Lucas A.},
